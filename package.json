    "reset-db": "tsc database/resetDatabase.ts --outDir .initDb && node .initDb/resetDatabase.js",
    "lint": "next lint"
  },
  "dependencies": {
    "@types/node": "18.14.6",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "dotenv": "^16.0.3",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.3",
    "knex": "^2.4.2",
    "next": "13.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.58.3",
    "sqlite3": "^5.1.4",
    "typescript": "4.9.5"
  }
}
